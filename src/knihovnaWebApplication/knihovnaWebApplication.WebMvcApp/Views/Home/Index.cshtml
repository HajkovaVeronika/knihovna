@{
    ViewData["Title"] = "Home Page";
}
@model List<Library>;

<head>
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
</head>


<div class="myContainer">
    <h2>Branches</h2>

    @foreach (var branch in Model) {
    <div class="branchContainer">
        <div class="branch">
            <p>@branch.Name</p>
            <div class="myBtns">
                <div class="btnRename myBtn">Rename</div>
                <div class="btnDetail myBtn">Detail</div>
            </div>
        </div>
    </div>
    }

</div>